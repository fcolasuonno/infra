server {
    {% include "nginx/internal.j2" %}
    {% include "nginx/params.j2" %}
    server_name {{ web_applications['tinymediamanager']['url'] }};


    location / {
        {% include "nginx/cors.j2" %}
        include /config/nginx/proxy.conf;
        include /config/nginx/resolver.conf;
        set $upstream_app {{ web_applications['tinymediamanager']['ip_address'] }};
        set $upstream_port 5800;
        set $upstream_proto http;
        proxy_pass $upstream_proto://$upstream_app:$upstream_port;

    }

    location ~ (/tinymediamanager)?/websockify {
         include /config/nginx/proxy.conf;
         include /config/nginx/resolver.conf;
         set $upstream_app {{ web_applications['tinymediamanager']['ip_address'] }};
         set $upstream_port 5800;
         set $upstream_proto http;
         proxy_pass $upstream_proto://$upstream_app:$upstream_port/websockify/;
     }

}

